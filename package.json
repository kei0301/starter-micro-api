{
  "name": "nothash-backend",
  "version": "1.0.0",
  "description": "nothash backend",
  "scripts": {
    "build": "tsc",
    "watch-tsc": "tsc -w",
    "deploy": "node dist/src/index.js",
    "watch-deploy": "nodemon dist/src/index.js",
    "server": "concurrently \"npm run watch-tsc\" \"npm run watch-deploy\"",
    "start": "npm run deploy",
    "dev": "nodemon src/index.ts --exec ts-node",
    "nfts": "nodemon src/nfts.ts --exec ts-node",
    "heroku-postbuild": "npm run tsc"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@types/cors": "^2.8.13",
    "@types/multer": "^1.4.7",
    "algosdk": "^2.4.0",
    "axios": "^1.2.1",
    "bcrypt-nodejs": "^0.0.3",
    "config": "^3.3.8",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-joi-validation": "^5.0.1",
    "fs": "^0.0.1-security",
    "gravatar": "^1.8.2",
    "http-status-codes": "^2.2.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mongoose": "^6.8.0",
    "multer": "^1.4.5-lts.1",
    "request": "^2.88.2",
    "socket.io": "^4.7.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "uuid": "^9.0.1",
    "web3": "^1.8.1"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "^0.0.31",
    "@types/config": "3.3.0",
    "@types/express": "^4.17.15",
    "@types/gravatar": "^1.8.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mongoose": "^5.11.97",
    "@types/paypal-rest-sdk": "^1.7.6",
    "@types/uuid": "^9.0.6",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  }
}
